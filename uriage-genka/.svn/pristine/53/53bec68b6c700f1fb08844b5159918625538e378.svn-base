.body {
    margin-left:188px;
}
.menu {
    background:#FC0;
    padding:0;
    position:absolute;
    width:120px;
    top:145px;
}
.row_odd {
    background-color: #EEFF8B;
}
.row_even {
  background-color: white;
}
.row1{background-color: white;}
.row2{background-color: aliceblue;}
.selected{background-color: yellow;}
.required{background-color: pink;}
div.header{
	position: fixed;
	z-index: 9999;
	width: 100%;
	top: 0;
	left: 0;
	padding: 0 10px;
	background-color: blue;
	color: white;
	font-weight: bold;
	font-size: medium;
}
.message{
	width: 100%;
	background-color: yellow;
	color: black;
	font-size: medium;
	font-weight: bold;
	padding: 0 10px;
}
div.fixed{position: fixed; z-index: 9999; width:100%; left:0; }

div.hide-input-form{padding-left:15px;}
div.input-form{
	background-color:#f8e58c;
	width:7500px;
	left:0;
	padding:5px 30px;
}
div.input-form table{background-color: transparent; border: none; padding-top: 5px;}
div.input-form th{width: 100px;}
div.input-form td{width: 100px;}
div.input-form font{font-weight:bold;}

div.hide-search-form{padding-left:15px;}
div.search-form{
	background-color:#afeeee;
	width:7500px;
	left:0;
	padding:5px 30px;
}
div.search-form table{background-color: transparent; border: none; padding-top: 5px;}
div.search-form th{width: 100px;}
div.search-form td{width: 100px;}
div.search-form{font-weight:bold;}


input[type="text"]{width:100px;}
select{
	width: 100px;
	vertical-align: middle;
	font-size: 15px;
}
div.list-header{position: fixed; z-index: 9999; width:2000px;}
div.list-body{position: absolute; width: 2000px;}
table.list{
	table-layout: fixed;
	border-collapse: collapse;
	border: solid thin black;
}
table.list td {
	min-width: 50px;
	padding-left: 5px;
	padding-right: 5px;
	border: solid thin black;
	z-index: 5000;
}
.theme-no{width: 100px;}
.theme-group{width: 100px;}
.name{width: 300px;}
.other{width: 50px}
.limited-auth{width: 50px;}
.date{width: 75px;}
.money{
	min-width: 50px;
	text-align: right;
}
.percentage{width: 75px;}

table.uk-input-table{
	table-layout: fixed;
	border: solid thin black;
}
table.uk-input-table th{width:75px;}
table.uk-input-table th.disabled{
	background-color:mediumblue;
	color: white;
}
table.uk-input-table td{width:75px;}
table.uk-input-table [type="text"]{width:75px;}
td.month{width:75px}

tr.blunk td,
table.list tr.blunk td {
	border : none;
	height: 5px;
}
tr.blunk,
table.list tr.blunk {
	border : none;
}
td.blunk,
table.list td.blunk {
	border : none;
	height: 5px;
}