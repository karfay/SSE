package ug.dto;


/**
 * 原価管理表の表示情報を受け取るクラス
 * テーマNO、テーマ親番、受注額、顧客名などを一度に受けたいため、複合的な容れ物が必要だった
 * @author sse802563
 *
 */
public class GkDto {
	//GenkaKanriエンティティの容れ物
    /** gkIdプロパティ */
    public String gkId;

    /** themeGroupプロパティ */
    public String themeGroup;

    /** themeNameプロパティ */
    public String themeName;

    /** kokyakuInitialプロパティ */
    public String kokyakuInitial;

    /** pjAdminプロパティ */
    public String pjAdmin;

    /** pjLeaderプロパティ */
    public String pjLeader;

    /** createrプロパティ */
    public String creater;

    /** tyakusyuMonthプロパティ */
    public String tyakusyuMonth;

    /** noukiプロパティ */
    public String nouki;

    /** lastUpdaterプロパティ */
    public String lastUpdater;

    /** lastUpdateTimeプロパティ */
    public String lastUpdateTime;

    //その他
    /** テーマNO　基本的にCONCATされた複数のテーマNOが格納される */
    public String themeNo;

    /** 受注額 */
    public String jutyugaku;

    public String createrName;

    public String pjLeaderName;

    public String pjAdminName;

    public String lastUpdaterName;
}
