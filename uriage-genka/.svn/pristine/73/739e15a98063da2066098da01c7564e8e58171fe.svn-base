    K_GAITYU decimal(14,0),
    K_RYOHI decimal(14,0),
    K_SONOTA decimal(14,0),
    K_KEIHI decimal(14,0),
    K_SUM decimal(14,0),
    M_ROUMUHI decimal(14,0),
    M_KOUSU decimal(5,1),
