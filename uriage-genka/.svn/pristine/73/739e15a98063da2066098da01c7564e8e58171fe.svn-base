    M_GAITYU decimal(14,0),
    M_RYOHI decimal(14,0),
    M_SONOTA decimal(14,0),
    M_KEIHI decimal(14,0),
    M_SUM decimal(14,0),
    J_ROUMUHI decimal(14,0),
    J_KOUSU decimal(5,1),
    J_TANKA decimal(14,0),
    J_GAITYU decimal(14,0),
    J_RYOHI decimal(14,0),
    J_SONOTA decimal(14,0),
    J_KEIHI decimal(14,0),
    J_SUM decimal(14,0),
    K_M_GAP decimal(14,0),
    K_J_GAP decimal(14,0),
