		//挿入用エンティティを宣言
		AnkenRireki ankenRireki = new AnkenRireki();
		Anken anken = new Anken();
		UriageKeikaku tounendo = new UriageKeikaku();
		UriageKeikaku jinendo = new UriageKeikaku();

		//案件テーブルを新規作成
